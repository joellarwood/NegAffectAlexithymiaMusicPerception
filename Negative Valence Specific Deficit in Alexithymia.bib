  pages = {142--150},
  keywords = {Alexithymia,Depression,CLINICAL,PSYCHOLOGY,ADOLESCENTS,Cognitive bias,FRONTAL EEG,GRIEF,MEMORY,MOOD,Music therapy,REHABILITATION,Sad music,THERAPY},
  author = {Bodner, Ehud and Iancu, Iulian and Gilboa, Avi and Sarel, Amiram and Mazor, Avi and Amir, Dorit},
